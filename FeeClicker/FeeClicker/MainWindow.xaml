<Window x:Class="FeeClicker.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FeeClicker"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="Resources/image.png"/>
        </Grid.Background>
        <Label Content="Fée clicker" HorizontalAlignment="Left" Margin="170,10,0,0" VerticalAlignment="Top" Height="68" Width="194" FontSize="36"/>
        <Button Content="Continuer partie" HorizontalAlignment="Left" Margin="190,102,0,0" VerticalAlignment="Top" Width="123" Height="47" Click="startSavedGame"/>
        <Button Content="Nouvelle partie" HorizontalAlignment="Left" Margin="190,192,0,0" VerticalAlignment="Top" Width="123" Height="47" Click="startNewGame"/>
        <Image x:Name="imageTest" HorizontalAlignment="Left" Height="116" Margin="59,139,0,0" VerticalAlignment="Top" Width="91" Source="Resources/image.png"/>

    </Grid>
</Window>
